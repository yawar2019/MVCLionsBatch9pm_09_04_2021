@model List<EmployeeModel>
@{
    ViewBag.Title = "Index4";
}
@using MVCLionsBatch9pm_09_04_2021.Models
<h2>Index4</h2>


<table class="table table-bordered">
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
        </tr>
    }
</table>